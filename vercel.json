{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./src/frameworks/express/index.js"
        }
    ],
    "env": {
        "MONGOURL": "MONGOURL",
        "DBNAME": "MONGOURL"
    }
}