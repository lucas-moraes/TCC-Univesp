{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/frameworks/express/index.js"
        }
    ],
    "env": {
        "MONGOURL": "MONGOURL",
        "DBNAME": "MONGOURL"
    }
}